			position absolute
			bottom 0
#section-4
	background color4
	//background-image radial-gradient(ellipse farthest-side at center, #FFFFFF 0%, #88C44A 100%)
	height 100%
	border-bottom .5em solid linea4
	.box-1
		.txt
			.txt-1
				font-family 'SansNarrowBold', 'sans-serif'
				font-size 6em
				color white
				text-shadow rgba(black, 0.2) 0.1em 0.1em 0.1em
	.info-companies
		.box-companies
			padding-top 2em
			.box-company	
				background white
				border 10px solid #EEE
				box-shadow 0 0 15px 5px rgba(gris_claro, 0.5)
				margin 3.95em
				height 40em
				padding 0	
				transition height 1s ease-out
				text-align center
				.website
					padding .5em
					height 2em
					a
						color white
						text-decoration none
				.site-1
					background azul_mas_oscuro	
				.site-2
					background rosado
				.site-3
					background rojo_etic										
				.description
					p
						padding 1em
				.tags
					position absolute
					bottom 0
					text-align center
					width 100%
					.tag
						border-radius 20%
						color white
						padding .1em	
			.company-1:hover
				animation 3s shadow-company-1 infinite ease-in-out
			.company-2:hover
				animation 3s shadow-company-2 infinite ease-in-out
			.company-3:hover
				animation 3s shadow-company-3 infinite ease-in-out
#section-5
	background color5
	height 100%
	border-bottom .5em solid linea5
	.box-1
		padding-top 24em
		.txt-1
			font-family "FrancoisOne", "sans-serif"
			text-shadow 3px 3px rgba(rojo_fuerte, 0.7)
			color color1
			font-size 6em
	.box-2
		margin-top 10em
		.pages
			background white
			height 20em
			margin-bottom 3em
			border 5px solid rgba(rojo_fuerte, 0.7)
			border-radius 15%
			img
				margin-top 3%
			.site-info-1
				height 8.7em
			.site-info-2
				height 7.7em	
			.site-info
				.description
					p
						color #3E3E3E;
				.tags
					text-align center
					.tag
						color #745C9E
						font-weight bold
		.pages:hover
			box-shadow 0 0 15px 5px rgba(black, 0.5)
		.link
			margin-top 1em
			a
				background #7842D8
				color white
				text-decoration none
				padding .5em
				border-radius 5px
				border-top-right-radius 5px
				border-bottom-right-radius 5px
				box-shadow inset 0 1px 0 #4722A5,0 5px 0 0 #4722A5,0 10px 5px transparent
#section-6
	background color1
	height 100%
	padding-top 20em
	.box-1
		color white
		font-size 2em
		font-family "MouseMemoirs", "sans-serif"
	.box-2
		a
			background #C52D2E
			color white
			text-shadow 3px 3px black
			font-family "MouseMemoirs", "sans-serif"
			text-decoration none
			font-size 4.5em
			border-radius 5px
			border-top-right-radius 5px
			border-bottom-right-radius 5px
			box-shadow inset 0 1px 0 #6D0000,0 5px 0 0 #6D0000,0 10px 5px transparent
			transition background 1s ease-out
			animation 3s contact-email infinite ease-in-out
			.span-mail-1
				padding 1.2em .2em 1.2em .2em
			.span-mail-2
				border-radius 0 3px 3px 0
				padding 0 .2em
				background #800
		a:hover
			background #C95152
	.shadow
		box-shadow 0 0 200px 150px rgba(0, 0, 0, 0.6)
		height 0
		position absolute
		top 50%
		animation 8s shadow-end infinite ease-in-out												
@keyframes float
	0%
		transform translate(0px, 0px)
	65%
		transform translate(0px, 30px)	
	100%
		transform translate(0px, 0px)												
@keyframes shadow-profile
	0%
		box-shadow 0 0 140px 100px rgba(0, 0, 0, 0.6)
	50%
		box-shadow 0 0 200px 100px rgba(0, 0, 0, 0.6)
	100%
		box-shadow 0 0 140px 100px rgba(0, 0, 0, 0.6)
@keyframes shadow-company-1
	0%
		box-shadow 0 0 95px 5px rgba(azul_mas_oscuro, 0.5)
	50%
		box-shadow 0 0 165px 35px rgba(azul_mas_oscuro, 0.5)
	100%
		box-shadow 0 0 95px 5px rgba(azul_mas_oscuro, 0.5)
@keyframes shadow-company-2
	0%
		box-shadow 0 0 95px 5px rgba(rosado, 0.5)
	50%
		box-shadow 0 0 165px 35px rgba(rosado, 0.5)
	100%
		box-shadow 0 0 95px 5px rgba(rosado, 0.5)
@keyframes shadow-company-3
	0%
		box-shadow 0 0 95px 5px rgba(rojo_etic, 0.5)
	50%
		box-shadow 0 0 165px 35px rgba(rojo_etic, 0.5)
	100%
		box-shadow 0 0 95px 5px rgba(rojo_etic, 0.5)
@keyframes shadow-end
	0%
		box-shadow 0 0 200px 150px rgba(0, 0, 0, 0.6)
	50%
		box-shadow 0 0 300px 250px rgba(0, 0, 0, 0.6)
	100%
		box-shadow 0 0 200px 150px rgba(0, 0, 0, 0.6)								
@keyframes rain 
	0%
		background-position 0px 0px
	100%
		background-position 500px 1000px, 400px 400px, 300px 300px
@keyframes contact-email
	0%
		background #C52D2E
	60%
		background #800
	100%
		background #C52D2E
