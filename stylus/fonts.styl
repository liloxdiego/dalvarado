@font-face {
	font-family: 'icomoon';
	src:url('../fonts/Icons/icomoon.eot?-rtae30');
	src:url('../fonts/Icons/icomoon.eot?#iefix-rtae30') format('embedded-opentype'),
		url('../fonts/Icons/icomoon.woff?-rtae30') format('woff'),
		url('../fonts/Icons/icomoon.ttf?-rtae30') format('truetype'),
		url('../fonts/Icons/icomoon.svg?-rtae30#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-profile:before {
	content: "\e600";
}
.icon-envelope:before {
	content: "\e601";
}
.icon-users:before {
	content: "\e602";
}
.icon-user:before {
	content: "\e603";
}
.icon-earth:before {
	content: "\e604";
}
.icon-link:before {
	content: "\e605";
}
